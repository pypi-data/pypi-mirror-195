#!python

"""
test-script
"""

import cdataml
import argparse

parser = argparse.ArgumentParser(add_help=True, formatter_class=argparse.RawDescriptionHelpFormatter, description = "test-script")
parser.add_argument("-p", "--print_me", help="String to run in test function", type=str, default="Hello World")
args = parser.parse_args()

def CallFunc(string):
    cdataml.test.testfunc(string)
    object = cdataml.TestClass(10)
    object.printobj()

if __name__ == '__main__':
    CallFunc(string=args.print_me)
